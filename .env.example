FLASK_ENV=production
SECRET_KEY=change-me-please

# ListenBrainz / MusicBrainz endpoints and user agents
LISTENBRAINZ_API=https://api.listenbrainz.org/1
MUSICBRAINZ_API=https://musicbrainz.org/ws/2
COVER_ART_API=https://coverartarchive.org/release
LISTENBRAINZ_USER_AGENT=spotify-wrapped-listenbrainz/1.0 (+https://github.com/devmatei/spotify-wrapped)
MUSICBRAINZ_USER_AGENT=spotify-wrapped-listenbrainz/1.0 (+https://github.com/devmatei/spotify-wrapped)
LISTENBRAINZ_RANGE=year

# Optional HTTP proxy support (requests respects these automatically when set)
#HTTP_PROXY=http://localhost:8080
#HTTPS_PROXY=http://localhost:8080
#NO_PROXY=localhost,127.0.0.1

# Last.fm integration (required for higher quality artist artwork)
LASTFM_API_KEY=your-lastfm-api-key
LASTFM_API=https://ws.audioscrobbler.com/2.0/
LASTFM_USER_AGENT=spotify-wrapped-listenbrainz/1.0 (+https://github.com/devmatei/spotify-wrapped)

# Performance and rate limiting
HTTP_TIMEOUT=10
LISTENBRAINZ_CACHE_TTL=60
LISTENBRAINZ_CACHE_SIZE=256
APP_RATE_LIMIT=90 per minute
APP_STATS_RATE_LIMIT=45 per minute
APP_IMAGE_RATE_LIMIT=15 per minute
APP_RATE_LIMIT_SALT=make-this-random
APP_TRUST_PROXY_HEADERS=0

# Gunicorn example (used by start.sh)
PORT=5000
